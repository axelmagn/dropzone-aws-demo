{ "expiration": "{{ expiration }}",
  "conditions": [
    {"bucket": "{{ bucket }}"},
    ["starts-with", "$key", "{{ upload_prefix }}"],
    {"acl": "{{ upload_acl }}"},
    {"success_action_redirect": "{{ redirect_url }}"},
    {"x-amz-credential": "{{ aws_credential }}"},
    {"x-amz-algorithm": "AWS4-HMAC-SHA256"},
    {"x-amz-date": "{{ aws_date }}" }
  ]
}
